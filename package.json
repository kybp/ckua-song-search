{
  "scripts": {
    "preinstall": "pip install -r requirements.txt",
    "postinstall": "webpack",
    "build": "webpack",
    "start": "python -m server",
    "test": "npm run test-server && npm run test-client",
    "test-client": "karma start",
    "test-server": "python -m unittest discover server/test/ '*_test.py'",
    "db-dump": "pg_dump ckua --no-owner > db.sql",
    "db-restore": "createdb ckua; psql ckua < db.sql",
    "save-date": "python server/save-date.py",
    "save-yesterday": "python server/save-date.py `date -d yesterday +%Y-%m-%d`"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "pickadate": "^3.5.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3"
  }
}
